{
    "name": "thecodingmachine/tdbm-graphql",
    "description": "A class generator that will generate GraphQL types from your database schema (using thecodingmachine/graphqlite).",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com"
        }
    ],
    "require": {
        "thecodingmachine/tdbm": "~5.3.0",
        "symfony/filesystem": "^2.0 || ^3.0 || ^4.0 || ^5.0",
        "psr/container": "^1",
        "thecodingmachine/graphqlite": "^4",
        "thecodingmachine/tdbm-fluid-schema-builder": "^1",
        "doctrine/dbal": "^2.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.14",
        "satooshi/php-coveralls": "^1.1",
        "mouf/picotainer": "^1.1",
        "symfony/cache": "^4.2.4 || ^5"
    },
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\Tdbm\\GraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\Tdbm\\GraphQL\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
